<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewmodel="clr-namespace:MauiMenuApp.Mobile.ViewModels"             
             xmlns:model="clr-namespace:MauiMenuApp.Domain.Models;assembly=MauiMenuApp.Domain"
             xmlns:controls="clr-namespace:MauiMenuApp.Mobile.Pages.Controls"
             x:Class="MauiMenuApp.Mobile.Pages.SubMenuPage"
             Title="SubMenuPage">
    <Grid RowDefinitions="*">
        <controls:MenuItemControl Grid.Row="0"
                                  MenuItems="{Binding SubMenuItems.MenuItems}"/>
    </Grid>
</ContentPage>