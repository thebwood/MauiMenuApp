<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiMenuApp.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:sf="clr-namespace:Syncfusion.Maui.Toolkit.SegmentedControl;assembly=Syncfusion.Maui.Toolkit"
    xmlns:pages="clr-namespace:MauiMenuApp.Mobile.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiMenuApp.Mobile">

    <ShellContent
        Title="Dashboard"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="main" />

    <ShellContent
        Title="Projects"
        ContentTemplate="{DataTemplate pages:SubMenuPage}"
        Route="SubMenus" />


</Shell>
